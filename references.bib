@Article{Johnson14,
  author =   {Johnson, Mathew A. and Noble, Pascal and Rodrigues, L. Miguel and Zumbrun, Kevin},
  title =    {Behavior of periodic solutions of viscous conservation laws under localized and nonlocalized perturbations},
  journal =  {Inventiones mathematicae},
