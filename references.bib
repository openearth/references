@Article{Molnar04,
  author   = {Peter Molnar},
  title    = {LATE CENOZOIC INCREASE IN ACCUMULATION RATES OF TERRESTRIAL SEDIMENT: How Might Climate Change Have Affected Erosion Rates?},
  doi      = {10.1146/annurev.earth.32.091003.143456},
  eprint   = {http://dx.doi.org/10.1146/annurev.earth.32.091003.143456},
