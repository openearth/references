  date         = {1988},
  journaltitle = {Journal of Engineering Mechanics},
  title        = {Hyperbolic Model for Aggrading Channels},
  doi          = {10.1061/(ASCE)0733-9399(1988)114:7(1245)},
  eprint       = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%290733-9399%281988%29114%3A7%281245%29},
