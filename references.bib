@Article{Furbish12_3,
  author =    {Furbish, David Jon and Ball, Ashley E. and Schmeeckle, Mark W.},
  title =     {A probabilistic description of the bed load sediment flux: 4. {F}ickian diffusion at low transport rates},
  journal =   {J. Geophys. Res., Earth Surface},
  year =      {2012},
  volume =    {117},
  number =    {F3},
  pages =     {F03034},
  abstract =  {High-speed imaging of coarse sand particles transported as bed load reveals how particle motions possess intrinsic periodicities associated with their start-and-stop behavior. The dominant harmonics in these motions have a primary influence on the rate at which the mean squared particle displacementR(!!e!!) !!e!! a measure conventionally used to assess the possibility of anomalous diffusion !!e!! increases with the time interval !!e!!. Over a timescale corresponding to the typical travel time of particles, calculations of R(!!e!!) may ostensibly indicate non-Fickian behavior while actually reflecting the effects of periodicities in particle motions, not anomalous diffusion. We provide the theoretical basis for this observed behavior, and we illustrate how the effective (Fickian) particle diffusivity obtains from G. I. Taylor's classic definition involving the particle velocity autocovariance, including its relation to the ensemble-averaged particle velocity as articulated by O. M. Phillips. Cross-stream diffusivities are an order of magnitude smaller than streamwise diffusivities.},
  doi =       {10.1029/2012JF002356},
  issn =      {2156-2202},
  publisher = {Wiley Online Library},
url =       {http:https://dx.doi.org/10.1029/2012JF002356},
}
