@Article{Parker78_1,
  author    = {Gary Parker},
  title     = {Self-formed straight rivers with equilibrium banks and mobile bed. {P}art 1. {T}he sand-silt river},
  doi       = {10.1017/S0022112078002499},
  number    = {1},
  pages     = {109--125},
  url       = {https://www.cambridge.org/core/article/self-formed-straight-rivers-with-equilibrium-banks-and-mobile-bed-part-1-sand-silt-river/68BE24A1DDFCC3B3A960EB61610C05B2},
  volume    = {89},
  abstract  = {Rivers and canals with perimeters composed of non-cohesive sand and silt have self-formed active beds and banks. They thus provide a most interesting fluid flow problem, for which one must determine the container as well as the flow. If bed load alone occurs across the perimeter of a wide channel, gravity will pull particles down the lateral slope of the banks; bank erosion is accomplished and the channel widens. In order to maintain equilibrium, this export of material from the banks must be countered by an import of sediment from the channel centre.The mechanism postulated for this import is lateral diffusion of suspended sediment, which overloads the flow near the banks and causes deposition. The model is formulated analytically with the aid of a series of approximate but reasonable assumptions. Singular perturbation techniques are used to define the channel geometry and obtain rational regime relations for straight channels. A comparison with data lends credence to the model.It is hoped that a first step has been made towards a more general treatment, which would include various complicating factors that are important features of natural rivers but are not essential to the maintenance of channel width. Among these factors are meandering, sediment sorting and seepage.}, 
  address   = {Cambridge, UK},
  day       = {14},
  journal   = {J Fluid Mech},
  month     = {11},
  owner     = {victorchavarri},
  publisher = {Cambridge University Press, Cambridge, United Kingdom},
  timestamp = {2016.11.30},
  year      = {1978},
}
@Article{Parker78_2,
  author       = {Parker, Gary},
  title        = {Self-formed straight rivers with equilibrium banks and mobile bed. {P}art 2. {T}he gravel river},
  doi          = {10.1017/S0022112078002505},
  number       = {1},
  pages        = {127--146},
  url          = {https://www.cambridge.org/core/article/self-formed-straight-rivers-with-equilibrium-banks-and-mobile-bed-part-2-gravel-river/3AD9322C1939528ED73D409654E35E22},
  volume       = {89},
  abstractnote = {Rivers are capable of transporting their own bed material without altering their width. However, a naive extension of the threshold theory of canals in coarse alluvium to straight reaches of gravel rivers leads to the stable-channel paradox: transport of bed material is incompatible with a stable width. In this paper singular perturbation techniques are used to obtain a bed stress distribution which allows a mobile bed but immobile banks at bankfull or dominant discharge. This result is used to obtain regime relations for straight rivers with bed and banks composed of coarse gravel.The analysis, although dependent on a series of approximate assumptions for Reynolds-stress closure and sediment transport, provides reasonable agreement with data.},
