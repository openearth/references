@Article{Larsson86,
  author       = {Rolf Larsson},
  date         = {1986},
  journaltitle = {Journal of Hydraulic Engineering},
  title        = {Coriolis Generated Secondary Currents in Channels},
  doi          = {10.1061/(ASCE)0733-9429(1986)112:8(750)},
  eprint       = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%290733-9429%281986%29112%3A8%28750%29},
  number       = {8},
  pages        = {750-767},
  url          = {https://ascelibrary.org/doi/abs/10.1061/%28ASCE%290733-9429%281986%29112%3A8%28750%29},
  volume       = {112},
  abstract     = {The effects of rotation on turbulent channel flow are discussed. In particular, the influence of the earth's rotation is analyzed, and some of the previous literature is reviewed. It is shown that Coriolis‐induced secondary currents should be of importance if they have a magnitude of 1\% of the downstream velocity. A simplified analysis is used to estimate the relative magnitude of the horizontal secondary velocity at the cross‐plane center point for fully developed flow in a wide channel as a function of Rossby number. Numerical model simulations are used to investigate the influence of the side walls and the magnitude of the secondary velocities for developing flow. The results show that the secondary velocities amount to about 1\% of the downstream velocity for rather slowly flowing deep channels.},
year =  {1986},
journal =  {Journal of Hydraulic Engineering},
}
