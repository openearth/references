  title    = {Turbulent Structure in Compound Open-Channel Flows},
  doi      = {10.1061/(ASCE)0733-9429(1991)117:1(21)},
  eprint   = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%290733-9429%281991%29117%3A1%2821%29},
