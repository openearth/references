@Article{Nezu84,
  author   = {Iehisa Nezu and Hiroji Nakagawa},
  title    = {Cellular Secondary Currents in Straight Conduit},
  doi      = {10.1061/(ASCE)0733-9429(1984)110:2(173)},
  eprint   = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%290733-9429%281984%29110%3A2%28173%29},
  number   = {2},
  pages    = {173-193},
  url      = {https://ascelibrary.org/doi/abs/10.1061/%28ASCE%290733-9429%281984%29110%3A2%28173%29},
  volume   = {110},
  abstract = {An investigation of cellular secondary currents, i.e., threeâ€�dimensional flow patterns, is very important in hydraulic engineering because these currents might cause threeâ€�dimensional sediment distributions and bed configurations such as sand ribbons in straight rivers. It is, however, fairly difficult to measure secondary currents of water flows, even by making use of hotâ€�film anemometers, because the velocity of secondary currents is within 5\% of the mainstream velocity. Thus, on the basis of the hypothesis that the existence of free surface may not be an essential cause of secondary currents, the present study has investigated experimentally the turbulent structure of their currents in air conduit. Considering an essential interaction between secondary currents and bedform, longitudinal ridge elements were attached onto both lower and upper bottoms of the conduit, which simulated the longitudinal ridges and trough of river bedform, i.e., sand ribbons. All three components of the velocity were measured accurately by hotâ€�wire anemometers. The structure of secondary currents was examined through the equations of mean flow vorticity and mean flow energy.},
  journal  = {J. Hydraul. Eng.},
  year     = {1984},
}
