@Article{Federici03,
  author   = {Federici, Bianca and Paola, Chris},
  title    = {Dynamics of channel bifurcations in noncohesive sediments},
  doi      = {10.1029/2002WR001434},
  issn     = {1944-7973},
  number   = {6},
  pages    = {1162},
  url      = {http://dx.doi.org/10.1029/2002WR001434},
  volume   = {39},
  abstract = {Braided rivers produce flow confluences and divergences (diffluences) in roughly equal numbers, but braided river research has focused mainly on confluences. Divergences, however, are of equal importance dynamically: they are sites of formation of central bars and the development of flow divisions that can steer the flow toward one part of the braided network or another. In a series of experiments on flow in isolated, well-defined bifurcations, we found that although a central bar always develops, the divided flow may continue to flow on both sides of the bar (!!e!!stable bifurcation) or may eventually be forced entirely to one side of the bar or the other (!!e!!unstable!!e!! bifurcation). We found that an unstable bifurcation forms when the flow field is characterized by both a low Shields stress and a nonuniform incoming flow. We also found that divergences with erodible banks tend to an equilibrium configuration that depends mainly on the widening ratio of the channel.},
  journal  = {Water Resour Res},
  keywords = {Erosion, Sedimentation, Streamflow, Instruments and techniques: modeling, Instruments and techniques: monitoring, braiding, channel bifurcations, stability, bars, bank erosion},
  year     = {2003},
}
