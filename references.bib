@Article{Slingerland04,
  author   = {Rudy Slingerland and Norman D. Smith},
  title    = {River Avulsions and their Deposits},
  doi      = {10.1146/annurev.earth.32.101802.120201},
  eprint   = {https://doi.org/10.1146/annurev.earth.32.101802.120201},
