@InProceedings{CastroBolinaga16,
  author    = {C. F. Castro-Bolinaga and P. Diplas and R. J. Bodnar},
  title     = {Dynamic vs. quasi-steady modeling of morphodynamic processes: A physics-based selection criterion},
  booktitle = {River Flow 2016},
  year      = {2016},
  abstract  = {The derivation, application, and validation of a physics-based criterion for selecting between the decoupled (quasi-steady) approach and the coupled (dynamic) approach for modeling morphodynamic processes in alluvial rivers are provided in this paper. A dimensionless parameter ?, which depends on the dynamic relationship between the Froude (Fr) number and the depth-averaged volumetric concentration of total sediment load (Ct), is proposed to represent the relative magnitude between the hydrodynamic and morphodynamic time scales, and therefore, establish a range of applicability of these approaches. It is envisioned that the physics-based criterion presented herein will provide a better understanding of how the ratio of the aforementioned time scales constrains the development and application of morphodynamic numerical models, as well as contribute towards improving assessment and prediction methods that are currently used for evaluating the impact of large-scale practical engineering projects on river corridors (e.g. dam decommissioning and mining-related activities).}, 
}
@Article{CastroDiaz08,
  author    = {M. J. Castro D\'iaz and E. D. Fern\'andez-Nieto and A. M. Ferreiro},
  title     = {Sediment transport models in Shallow Water equations and numerical approach by high order finite volume methods},
  doi       = {http://dx.doi.org/10.1016/j.compfluid.2007.07.017},
  issn      = {0045-7930},
  number    = {3},
  pages     = {299--316},
  url       = {http://www.sciencedirect.com/science/article/pii/S0045793007001454},
  volume    = {37},
  abstract  = {This paper is concerned with the numerical approximation of bedload sediment transport due to water evolution. For the hydrodynamical component we consider Shallow Water equations. The morphodynamical component is defined by a continuity equation, which is defined in function of the solid transport discharge. We present several deterministic models, such as Meyer-Peter \&amp; M!!e!!ller, Van Rijn or Grass model. We also present an unified definition for the solid transport discharge, and we compare with Grass model. Both components define a coupled system of equations that can be rewrite as a non-conservative hyperbolic system. To discretize it, we consider finite volume methods with or without flux limiters and high order state reconstructions. Finally we present several tests, where we observe numerically the order of the numerical schemes. Comparisons with analytical solutions and experimental data are also presented.}, 
