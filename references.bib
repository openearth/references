@Article{BollaPittaluga15_2,
  author   = {Bolla Pittaluga, Michele and Coco, Giovanni and Kleinhans, Maarten G.},
  title    = {A unified framework for stability of channel bifurcations in gravel and sand fluvial systems},
  doi      = {10.1002/2015GL065175},
  issn     = {1944-8007},
  number   = {18},
  pages    = {7521--7536},
  url      = {http://dx.doi.org/10.1002/2015GL065175},
  volume   = {42},
  abstract = {Bifurcating rivers shape natural landscapes by distributing water and sediments on fluvial plains and in deltas. Symmetrical bifurcations were often found to be unstable so that one branch downstream of the bifurcation enlarged while the other dwindled. A unified theory able to predict bifurcation stability in both gravel bed and sand bed rivers is still lacking. Here we develop a new theory for the stability of bifurcations for the entire range of gravel bed to sand bed rivers. The theory indicates opposite behavior of gravel bed and sand bed rivers: we predict that symmetrical bifurcations are inherently stable for intermediate Shields stresses but are inherently unstable for the low and high Shields stresses found in the majority of rivers on Earth. In the latter conditions asymmetrical bifurcations are stable. These predictions are corroborated by observations and have ramifications for many environmental problems in fluviodeltaic settings.},
  journal  = {Geophys Res Lett},
  keywords = {Geomorphology: fluvial, River channels, Sedimentation, Sediment transport, Modeling, bifurcation, stability, river, delta, morphodynamic},
  year     = {2015},
}
