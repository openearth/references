@Article{Castro09,
  author   = {Castro, M. J. and Fern{\'a}ndez-Nieto, E. D. and Ferreiro, A. M. and Garc{\'i}a-Rodr{\'i}guez, J. A. and Par{\'e}s, C.},
  title    = {High Order Extensions of {R}oe Schemes for Two-Dimensional Nonconservative Hyperbolic Systems},
