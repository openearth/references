@Article{Sonke03,
  author   = {Jeroen E. Sonke and David Jon Furbish and Vincent J. M. Salters},
  title    = {Dispersion effects of laminar flow and spray chamber volume in capillary electrophoresis-inductively coupled plasma-mass spectrometry: a numerical and experimental approach},
  doi      = {10.1016/S0021-9673(03)01210-X},
  issn     = {0021-9673},
