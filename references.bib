@Article{Roos07,
  author   = {Roos, Pieter C. and Wemmenhove, Rik and Hulscher, Suzanne J. M. H. and Hoeijmakers, Harry W. M. and Kruyt, N. P.},
  title    = {Modeling the effect of nonuniform sediment on the dynamics of offshore tidal sandbanks},
  doi      = {10.1029/2005JF000376},
  eprint   = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2005JF000376},
  number   = {F2},
  url      = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2005JF000376},
  volume   = {112},
  abstract = {Tidal sandbanks are large-scale bed features present in many shallow shelf seas. Here we investigate the effect of nonuniform sediment on their dynamics, with a particular aim to explain observed surficial grain size variations over tidal sandbanks from a process-based modeling perspective. To this end, we use a linear stability analysis that describes the positive feedback mechanism between hydrodynamics, sediment, and the seabed responsible for sandbank formation on a horizontal shelf. In this model the sediment transport and bed evolution modules are extended by introducing an active layer and a bimodal sediment mixture. We include a dynamic hiding/exposure description of sediment transport, enhancing the transport of coarse grains and inhibiting the transport of finer grains. The model results show that for symmetrical tidal conditions, coarse grains tend to accumulate at the bank crests. Moreover, the growth rates of the perturbations increase compared to the case of uniform sediment, while the preferred wavelength and bank orientation remain unchanged. For asymmetrical tidal conditions we find a spatial phase shift between topography and the mean grain size fraction, indicating an accumulation of coarse grains on the lee side of the bank. The model results qualitatively agree with observations from banks on the Belgian continental shelf.},
  journal  = {J. Geophys. Res., Earth Surface},
  keywords = {morphodynamic modeling, nonuniform sediment, tidal sandbanks},
  year     = {2007},
}
@Article{vanGerwen18,
  author   = {{\van{Gerwen}{Van}{van}} Gerwen, W. and B. W. Borsje and J. H. Damveld and S. J. M. H. Hulscher},
  title    = {Modelling the effect of suspended load transport and tidal asymmetry on the equilibrium tidal sand wave height},
  doi      = {https://doi.org/10.1016/j.coastaleng.2018.01.006},
  issn     = {0378-3839},
  pages    = {56 - 64},
  url      = {http://www.sciencedirect.com/science/article/pii/S0378383917301667},
  volume   = {136},
  abstract = {Tidal sand waves are rhythmic bed forms found in shallow sandy coastal seas, reaching heights up to ten meters and migration rates of several meters per year. Because of their dynamic behaviour, unravelling the physical processes behind the growth of these bed forms is of particular interest to science and offshore industries. Various modelling efforts have given a good description of the initial stages of sand wave formation by adopting a linear stability analysis on the coupled system of water movement and the sandy seabed. However, the physical processes causing sand waves to grow towards equilibrium are far from understood. We adopt a numerical shallow water model (Delft3D) to study the growth of sand waves towards a stable equilibrium. It is shown that both suspended load transport and tidal asymmetry reduce the equilibrium sand wave height. A residual current results in asymmetrical bed forms that migrate in the direction of the residual current. The combination of suspended load transport and tidal asymmetry results in predicted equilibrium wave heights comparable to wave heights found in the field.},
  journal  = {Coastal Eng},
  keywords = {Morphodynamic modelling, Tidal sand waves, Equilibrium height, Delft3D, North Sea},
  year     = {2018},
}
@InBook{Mosselman09,
  chapter =   {Intrinsic steady alternate bars in alluvial channels. Part 2: Theoretical analysis},
  pages =     {767--772},
  title =     {Proceedings of the 6th Symposium on River Coastal and Estuarine Morphodynamics, 21-25 September, Santa Fe, Argentina.},
  publisher = {CRC Press},
  year =      {2009},
  author =    {E. Mosselman},
  editor =    {Vionnet, C. A. and Garcia, M. H. and Latrubesse, E. M. and Perillo, G. M. E.},
  abstract =  {Alternate bars in straight alluvial channels are migrating or steady. The currently accepted view is that they are steady only if the width-to-depth ratio is at the value of resonance or if the bars are forced by a steady local perturbation. Experimental observations, however, show that steady bars are also present in cases of migrating bars without permanent forcings. The companion paper by Crosato  \&  Desta (2009) reviews these experimental observations and presents a numerical analysis using a 2D depth-averaged morphological model. Here we explain the occurrence of steady bars using a 2D linear stability analysis. The temporal growth rate of steady bars is zero under constant conditions, but is found to become positive if the conditions exhibit small temporal fluctuations. These fluctuations could result from discharge variations or the mere presence of migrating alternate bars. Since steady bars are seen as a prerequisite to explain meandering of alluvial rivers, these findings imply that neither resonant width-to-depth ratios nor steady local perturbations are necessary conditions for the onset of river meandering.}, 
}
