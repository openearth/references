@Article{Stoelum96,
  author       = {Hans-Henrik St{\o}lum},
  date         = {1996},
  journaltitle = {Science},
  title        = {River Meandering as a Self-Organization Process},
  doi          = {10.1126/science.271.5256.1710},
  eprint       = {https://www.science.org/doi/pdf/10.1126/science.271.5256.1710},
  number       = {5256},
  pages        = {1710--1713},
  url          = {https://www.science.org/doi/abs/10.1126/science.271.5256.1710},
  volume       = {271},
  abstract     = {Simulations of freely meandering rivers and empirical data show that the meandering process self-organizes the river morphology, or planform, into a critical state characterized by fractal geometry. The meandering process oscillates in space and time between a state in which the river planform is ordered and one in which it is chaotic. Clusters of river cutoffs tend to cause a transition between these two states and to force the system into stationary fluctuations around the critical state.},
journal =  {Science},
year =  {1996},
}





@Article{Begin81_2,
  author       = {Begin, Z. B.},
  date         = {1981},
  journaltitle = {The Journal of Geology},
  title        = {Stream Curvature and Bank Erosion: A Model Based on the Momentum Equation},
  doi          = {10.1086/628610},
  eprint       = {https://doi.org/10.1086/628610},
  number       = {4},
  pages        = {497--504},
  url          = {https://doi.org/10.1086/628610},
  volume       = {89},
  abstract     = {Based on the momentum equation of flow, the force per unit area which the flow exerts radially on the outer bank of a stream bend is expressed by an equation in which the force is explicitly related to the curvature ratio R/w (curve radius/channel width). A dimensionless curvature coefficient \$\$C\_{\alpha}\$\$ is defined, which is dependent on R/w and the coefficient of dynamic bedload friction, \$\$tan \alpha\$\$. Exploring the changes of \$\$C\_{\alpha}\$\$ with R/w reveals a functional relationship which, for the known values of \$\$tan \alpha\$\$, shows maximal values of \$\$C\_{\alpha}\$\$ for R/w values between 2 and 4. The theoretical results agree with field measurements of the relationship between R/w and the rate of channel migration.},
year =  {1981},
journal =  {The Journal of Geology},
}
