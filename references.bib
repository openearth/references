@Article{Bridge84,
  author   = {Bridge, J. S. and Dominic, D. F.},
  title    = {Bed Load Grain Velocities and Sediment Transport Rates},
  doi      = {10.1029/WR020i004p00476},
  eprint   = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/WR020i004p00476},
