  title    = {Analytical Determination of Form Friction Factor},
  doi      = {10.1061/(ASCE)0733-9429(1983)109:4(590)},
  eprint   = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%290733-9429%281983%29109%3A4%28590%29},
  number   = {4},
