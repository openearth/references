@Article{FoufoulaGeorgiou10,
  author =   {Efi Foufoula-Georgiou and Colin Stark},
  title =    {Introduction to special section on Stochastic Transport and Emergent Scaling on Earth's Surface: Rethinking geomorphic transport-stochastic theories, broad scales of motion and nonlocality},
  journal =  {J. Geophys. Res., Earth Surface},
  year =     {2010},
  volume =   {115},
  number =   {F2},
  pages =    {F00A01},
  abstract = {In many geomorphic transport systems, the time and length scales of motion vary widely: particles can be trapped for both short and long periods of time and they can travel large or small distances in very short intervals of time. To model such systems we need fresh conceptual and mathematical formalisms. The goal of this collection of papers is to challenge existing thinking in geomorphic transport by putting forward new ideas and theories for environmental fluxes, from particle transport in a single stream, to landslide debris mobilization, sediment and water transport on hillslopes, dynamic transport on river networks, and interpretation of sedimentary deposits over geologic time. Advanced stochastic theories of transport are proposed, the notion of nonlocal flux is introduced, and fractional advectionâ€�diffusion equations are explored as possible models of geomorphic transport.},
  doi =      {10.1029/2010JF001661},
  eprint =   {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2010JF001661},
  keywords = {geomorphic transport, stochastic theories, fractional advectionâ€�diffusion},
