@Article{Spinewine08,
  author =   {B. Spinewine and Y. Zech},
  title =    {An ex-post analysis of the German Upper Rhine: data gathering and numerical modelling of morphological changes in the 19th century},
  journal =  {J. Flood Risk Manage.},
  year =     {2008},
  volume =   {1},
  number =   {1},
  pages =    {57--68},
  abstract = {Abstract To ensure sustainability, longâ€�term effectiveness and limited maintenance, river flood management practices should take into consideration the potential morphological adaptations resulting from the imposed changes to a river system. To illustrate the ability of numerical models to provide a valuable tool for flood management planners, an exâ€�post analysis of the Rhine during the 19th century is carried out. The Upper German Rhine has been reshaped deeply by large engineering works over this century. The initially braided reach Baselâ€“Maxau was constricted into a single channel, while the reach Maxauâ€“Mainz was significantly shortened by meander shortcuts. Both types of works resulted in intense erosion upstream and spread deposition downstream, with most of the changes occurring during highâ€�flow periods. As ancient data are generally difficult to obtain, the paper shows how fragmentary ancient data may be complemented with more recent data adequately interpreted for reconstructing a realistic set of initial conditions. The study is an attempt at reproducing qualitatively the behaviour of the Upper Rhine with a dataset collected only from publicly available sources.},
  doi =      {10.1111/j.1753-318X.2008.00007.x},
  eprint =   {https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1753-318X.2008.00007.x},
  keywords = {Exâ€�post analysis, data gathering, flood management, longâ€�term numerical modelling, river morphology},
url =      {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1753-318X.2008.00007.x},
}
