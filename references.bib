@Article{Johannesson89,
  author   = {Helgi Johannesson and Gary Parker},
  title    = {Secondary Flow in Mildly Sinuous Channel},
  doi      = {10.1061/(ASCE)0733-9429(1989)115:3(289)},
  eprint   = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%290733-9429%281989%29115%3A3%28289%29},
  number   = {3},
  pages    = {289--308},
  url      = {https://ascelibrary.org/doi/abs/10.1061/%28ASCE%290733-9429%281989%29115%3A3%28289%29},
  volume   = {115},
  abstract = {Most analytical models of the flow and bed topography in sinuous channels include the assumption that the secondary flow is locally adapted to channel curvature. Recently, Kitanidis and Kennedy (1984) and Ikeda and Nishimura (1986) have, however, established the existence of a phase lag between the two, induced by inertia, i.e., downstream convective acceleration of the secondary flow. The analysis of Ikeda and Nishimura (1986), nevertheless, includes an unverified assumption. Herein a more rigorous derivation of the effect is obtained with a minimum of assumptions. The predicted lag appears to be sufficiently small to warrant neglect in natural channels. The theory predicts much higher values in many experimental channels, in agreement with data. The reason for this is that a scaled dimensionless meander wave number r, found to be orderâ€�one in natural channels, is found to be an order of magnitude higher in many laboratory meandering flumes.},
  journal  = {J. Hydraul. Eng.},
  year     = {1989},
}
@ARTICLE{Johansson76,
  author = {Johansson, Carl Erik},
  title = {Structural Studies of Frictional Sediments},
  journal = {Geografiska Annaler. Series A, Physical Geography},
  year = {1976},
  volume = {58},
  pages = {201--301},
  number = {4},
  abstract = {The reactions of pebbles, granules and sand grains to different flow -- sediment -- bed conditions under varying flow energy input are discussed and investigated theoretically and experimentally with special regard to the relaxation of sedimentary structures and their corresponding fabrics. Some aspects are given of dynamic conditions affecting the stability, erosion, transport and deposition of particles of different sizes and shapes in non-uniform frictional sediments under varying flow conditions. Different types of imbrication are related to particle geometry, flow velocities, flow separation, bed resistance to deformation, contacts of the particles, sediment transport, and time. Stability -- erodibility of the particles at varying orientation is shown to depend on the support, angle of flow attack, particle exposure, and fluctuations of flow directions and tractive force. The response of gravelly sand beds to the flow energy input by development of gravelly transport -- erosion surfaces, gradually stabilizing as imbricated pebble pavements, is demonstrated at different bed slopes under 2- and 3-dimensional geometry. Bed structures and forms of coarse-grained 2- and 3-dimensional delta bodies are related to the flow traction, flow separation, gravity pull, friction, and sediment segregation in backset, topset, foreset and bottomset beds. The results are shown in diagrams and tables. Conclusions and summaries are found at the end of each chapter. A key to interpret the sedimentary environment from textural and structural data is devised in Ch. 10.}, 
