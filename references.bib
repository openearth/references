@Article{LeaoFrigo19,
  author       = {Anderson Leão Frigo and Rolf-Dieter Zentgraf and Tobias Berward Bleninger},
  date         = {2019},
  journal      = {Journal of Waterway, Port, Coastal, and Ocean Engineering},
  title        = {Two-Dimensional Vessel-Current Interaction Model for Inland Waterways Assessment},
  doi          = {10.1061/(ASCE)WW.1943-5460.0000494},
  eprint       = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%29WW.1943-5460.0000494},
  number       = {1},
  pages        = {04018036},
  url          = {https://ascelibrary.org/doi/abs/10.1061/%28ASCE%29WW.1943-5460.0000494},
  volume       = {145},
  abstract     = {This study presents a computational tool for the analysis of commercial navigation capacity of rivers by a fast-time two-dimensional vessel–current interaction model. Different scenarios of rivers and vessels can be simulated, and the model can be used during early-stage design and overall management of inland fairways. The mathematical description relies on the 6DOF equations, where Eulerian flows and a Lagrangian ship-tracking approach is used, coupling river features and the main characteristics of ships. For each time step, the resultant linear and angular momenta were calculated through analytical equations to measure the ship movement and other relevant parameters such as velocity and drift angle. An autopilot system was also developed to reproduce the necessary maneuvers during navigation. A German section of the Rhine River was used for verification. The model reproduced the main movements of vessels in restricted regions; therefore, one can estimate the set of parameters and necessary adaptations in rivers and ships that optimize safe commercial navigation for waterways projects.},
year =  {2019},
}
