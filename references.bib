@Article{Decyk08,
  author   = {Viktor K. Decyk and Henry J. Gardner},
  title    = {Object-oriented design patterns in Fortran 90/95: mazev1, mazev2 and mazev3},
  doi      = {https://doi.org/10.1016/j.cpc.2007.11.013},
  issn     = {0010-4655},
