@Article{Stewart79,
  author   = {H. Bruce Stewart},
  title    = {Stability of two-phase flow calculation using two-fluid models},
  doi      = {10.1016/0021-9991(79)90020-2},
  issn     = {0021-9991},
  number   = {2},
