@Article{Blanckaert01,
  author   = {Koen Blanckaert and Walter H. Graf},
  title    = {Mean Flow and Turbulence in Open-Channel Bend},
  doi      = {10.1061/(ASCE)0733-9429(2001)127:10(835)},
  eprint   = {https://ascelibrary.org/doi/pdf/10.1061/%28ASCE%290733-9429%282001%29127%3A10%28835%29},
  number   = {10},
  pages    = {835--847},
  url      = {https://ascelibrary.org/doi/abs/10.1061/%28ASCE%290733-9429%282001%29127%3A10%28835%29},
  volume   = {127},
  abstract = {Flow over a developed bottom topography in a bend has been investigated experimentally. The measuring section is in the outer-bank half of the cross section at 60° into the bend. Spatial distributions of the mean velocities, turbulent stresses, and mean-flow and turbulent kinetic energy are presented. The cross-sectional motion contains two cells of circulation: besides the classical helical motion (center-region cell), a weaker counterrotating cell (outer-bank cell) is observed in the corner formed by the outer bank and the water surface. The downstream velocity in the outer half-section is higher than the one in straight uniform flow; the core of maximum velocities is found close to the separation between both circulation cells, well below the water surface. The turbulence structure in a bend is different from that in a straight flow, most notably in a reduction of the turbulent activity toward the outer bank. Both the outer-bank cell and reduced turbulent activity have a protective effect on the outer bank and the adjacent bottom and thus influence the stability of the flow perimeter and the bend morphology.},
  journal  = {Journal of Hydraulic Engineering},
  year     = {2001},
}
@InCollection{Tuijnder10,
  author    = {Arjan Tuijnder and Jan S. Ribberink},
  booktitle = {Proceedings of the 5th International Conference on Fluvial Hydraulics (River Flow), Braunschweig, Germany, 8-10 September},
  title     = {Development of supply-limited transport due to vertical sorting of a sand-gravel mixture},
  editor    = {Rafael {Murillo Mu\~noz}},
  pages     = {487--492},
  publisher = {CRC press, Taylor and Francis Group},
  abstract  = {Within the framework of the German research programme KLIWAS, a project was set up to develop applicable hydro-engineering and water management measures to sustain the navigability in the free flowing waterways even under extreme low flow conditions. Regulating structures developed within this framework will be optimised by means of a 2-dimensional hydro-/morphodynamic model. Beyond that, maintenance strategies are investigated aiming at the reduction of future maintenance costs under changed hydrological conditions, and leading to a large number of morphodynamic long-term simulations that have to be conducted. Obviously, techniques are eligible to reduce the computing time of these long-term morphodynamic computations. As the development of maintenance strategies depends on the prognoses of time and space dependent aggradations and degradations aswell as dredging and disposal events, the degree of a possible model abstraction is limited considerably.Within this study, two techniques for accelerating morphodynamic simulations are tested for their suitability with respect to computing time and accuracy.}, 
  year      = {2010},
}
